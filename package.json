  },
  "scripts": {
    "prepare": "husky install",
    "reinstall": "rimraf node_modules && pnpm i",
    "lint": "eslint . --ext .ts,.js,.cjs,.mjs,.vue --fix --cache --cache-location node_modules/.cache/eslint/",
    "stylelint": "stylelint \"**/*.{css,scss,vue}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
    "format": "prettier \"**/*.{js,ts,json,tsx,css,scss,vue,md}\" --write --cache --cache-location node_modules/.cache/prettier/.cache",
    "lint-staged": "lint-staged",
    "cz": "czg",
    "type-check": "vue-tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@iconify/tools": "^3.0.5",
