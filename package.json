    "@types/lodash": "^4.14.201",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "czg": "^1.7.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lerna": "^7.4.2",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "stylelint": "^15.11.0",
